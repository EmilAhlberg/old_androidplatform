<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.emil.app">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/dog_icon"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name="com.example.emil.Framework.ActivityHandler" />
        <activity android:name="com.example.emil.Framework.GameActivity" />
        <activity android:name="com.example.emil.Framework.LevelClearedActivity" />
        <activity android:name="com.example.emil.Framework.GameOverActivity" />
        <activity android:name="com.example.emil.Framework.StartMenuActivity" >
            com.example.emil.Framework.StartMenuActivity
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.emil.Framework.SelectLevelActivity" />
    </application>

</manifest>